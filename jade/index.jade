html
  head
    title 
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible" content="IE=edge,chrome=1")
    meta(name="viewport" content="width=device-width")
    link(rel="stylesheet" href="style.min.css")
    
  body
    nav
      .navbar
        .nav-header
          i(class="fa fa-times fa-2x menubtn" aria-hidden="true")
        .nav-body  
          h2.logo(href="http://www.avans.nl/") Avans Hogeschool
          h2 Informatica
          .divider
          ul
            li.nav-item
              a(href="#intro") 
                i(class="fa fa-home fa-fw" aria-hidden="true") 
                span Intro
            li.nav-item
              a(href="#over") 
                i(class="fa fa-info-circle fa-fw" aria-hidden="true") 
                span Over
            li.nav-item
              a(href="#leren") 
                i(class="fa fa-graduation-cap fa-fw" aria-hidden="true") 
                span Wat leer ik?
            li.nav-item
              a(href="#leerjaren")
                i(class="fa fa-pie-chart fa-fw" aria-hidden="true") 
                span Leerjaren
            li.nav-item
              a(href="#beroepsprofielen") 
                i(class="fa fa-briefcase fa-fw" aria-hidden="true") 
                span Beroepsprofielen
            li.nav-item
              a(href="#studiepunten") 
                i(class="fa fa-star fa-fw" aria-hidden="true") 
                span Studiepunten
            li.nav-item
              a(href="#faq")
                i(class="fa fa-question-circle fa-fw" aria-hidden="true")
                span FAQ
            li.nav-item
              a(href="#contact") 
                i(class="fa fa-user fa-fw" aria-hidden="true") 
                span Contact
      .overlay
    header.container-fluid.affix.affix-top
      .row
        .nav-menu.col-md-12
          .menubtn
            i(class="fa fa-bars fa-2x" aria-hidden="true")
          span Informatica
    div(id="intro" class="carousel slide hero-image" data-ride="carousel")
      ol(class="carousel-indicators")
        li(data-target="#intro" data-slide-to="0" class="active")
        li(data-target="#intro" data-slide-to="1")
        li(data-target="#intro" data-slide-to="2")
      div(class="carousel-inner" role="listbox")
        div(class="item active")
          img(src="img/avans1.png" alt="Avans Hogeschool 1")
        div(class="item")
          img(src="img/avans2.png" alt="Avans Hogeschool 2")
        div(class="item")
          img(src="img/avans3.png" alt="Avans Hogeschool 3")
        .hero-text
          h1 Informatica
          h3 Een opleiding voor jou?
